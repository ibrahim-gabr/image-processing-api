{
	"name": "image-processing-api",
	"version": "1.0.0",
	"description": "Image Processing API",
	"main": "src/index.ts",
	"scripts": {
		"dev": "ts-node-dev --respawn --clear --transpile-only --ignore-watch node_modules ./src/index.ts",
		"build": "rimraf ./dist && tsc",
		"start": "node dist/app.js",
		"lint": "eslint src/**/*.ts",
		"format": "prettier --config .prettierrc --write \"src/**/*.ts\"",
		"jasmine": "jasmine --config=src/spec/support/jasmine.json",
		"test": "npm run build && npm run jasmine"
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/express": "^4.17.21",
		"@types/jasmine": "^5.1.4",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.2",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-promise": "^6.0.0",
		"jest": "^29.7.0",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"supertest": "^7.0.0",
		"ts-jest": "^29.1.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "*"
	},
	"dependencies": {
		"body-parser": "^1.20.2",
		"compression": "^1.7.4",
		"dotenv": "^16.4.5",
		"ejs": "^3.1.10",
		"env-var": "^7.4.1",
		"express": "^4.19.2",
		"express-rate-limit": "^7.2.0",
		"jasmine": "^5.1.0",
		"jasmine-spec-reporter": "^7.0.0",
		"sharp": "^0.33.4"
	}
}
